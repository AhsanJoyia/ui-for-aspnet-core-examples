@(Html.Kendo().ImageEditor()
        .Name("imageEditor")
        .Height(900)
        .SaveAs(s => {
            s.FileName("image_edited.png");
            s.ForceProxy(true);
            s.ProxyURL(Url.Action("ImageEditorSave", "ImageEditorSave"));
        })
        .ImageUrl(@Url.Content("~/images/200.png"))
    )